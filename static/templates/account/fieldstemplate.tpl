<div id="fields-template-input">
    <input class="form-control" type="text" id="{name}" name="{name}" data-property="{name}" placeholder="{label}">
</div>
<div id="fields-template-radio">
  <div class="radio">
    <label>
      <input type="radio" id="{name}" name="{name}" data-property="{name}" value="{value}"> <strong>{label}</strong>
    </label>
  </div>
</div>
<div id="fields-template-checkbox">
  <div class="checkbox">
    <label>
      <input type="checkbox" id="{name}" name="{name}" data-property="{name}" value="{value}"> <strong>{label}</strong>
    </label>
  </div>
</div>
